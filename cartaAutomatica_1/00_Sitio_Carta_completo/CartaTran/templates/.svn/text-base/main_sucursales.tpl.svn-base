<SCRIPT language="javascript">
		function LoadImg(sImg, iTop, iLeft) {
			var oDiv = document.getElementById('Sucursal');
			var oImg = document.getElementById('imgSucursal');
			
			if(oImg != null) {
				oImg.src = sImg;
			}
			oDiv.style.visibility = 'visible';
			oDiv.style.display = 'block';
		}
</SCRIPT>
